{
  "name": "fe-engineer.com",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "start-remix": "PORT=3002 remix-serve ./build/index.js",
    "start": "pm2 start --name=fe --update-env pnpm -- remix",
    "remix": "pnpm i && pnpm build && pnpm start-remix",
    "stop": "pm2 delete fe",
    "reload": "git pull && pnpm build && pm2 restart fe --update-env",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@mantine/carousel": "^7.3.2",
    "@mantine/code-highlight": "^7.3.2",
    "@mantine/core": "7.3.2",
    "@mantine/dates": "^7.3.2",
    "@mantine/dropzone": "^7.3.2",
    "@mantine/form": "^7.3.2",
    "@mantine/hooks": "7.3.2",
    "@mantine/modals": "^7.3.2",
    "@mantine/notifications": "^7.3.2",
    "@mantine/tiptap": "^7.3.2",
    "@remix-run/css-bundle": "2.4.1",
    "@remix-run/node": "2.4.1",
    "@remix-run/react": "2.4.1",
    "@remix-run/serve": "2.4.1",
    "@tabler/icons-react": "2.44.0",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "embla-carousel-react": "8.0.0-rc17",
    "html-entities": "^2.4.0",
    "isbot": "^3.7.0",
    "node-cache": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "use-count-up": "^3.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "2.4.1",
    "@remix-run/eslint-config": "2.4.1",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.53.0",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "1.12.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm"
}
